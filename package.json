    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "chokidar": "1.4.2",
    "cli-color": "1.1.0",
    "cross-env": "^1.0.7",
    "css-loader": "0.23.1",
    "express": "4.13.4",
    "file-loader": "0.8.5",
    "fs-extra": "0.26.5",
