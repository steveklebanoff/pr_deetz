{
  "manifest_version": 2,
  "name": "PR Deetz",
  "homepage_url": "http://www.headynation.com",
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*/commit/*"],
      "js": ["github_pr.js"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "options_ui": {
    "page": "settings/settings.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "web_accessible_resources": ["settings/settings.html"]
}
